{
    "suggest.enablePreselect": false,
    "suggest.noselect": true,
    "typescript.suggestionActions.enabled": false,
    "javascript.suggestionActions.enabled": false,
    "python.venvFolders": [
        "venv"
    ],
    "python.linting.enabled": true,
    "python.jediEnabled": true,
    "python.linting.mypyEnabled": false,
    "python.linting.pep8Enabled": true,
    "python.formatting.autopep8Path": "autopep8",
    "css.format.enable": true,
    "htmldjango.formatting.provider": "djlint",
    "htmldjango.enable": true,
    "htmldjango.completion.enable": true,
    "htmldjango.djlint.commandPath": "djlint",
    "htmldjango.djlint.indent": 4,
    "html.filetypes": [
        "htmldjango",
        "djangohtml"
    ],
    "htmldjango.djlint.profile": "django",
    "htmldjango.djlint.enableLint": true,
    "htmldjango.djlint.formatCss": true,
    "htmldjango.djlint.formatJs": true,
    "htmldjango.djlint.lintOnChange": true,
    "html.format.enable": true,
    "html.format.indentInnerHtml": true,
    "html.hover.documentation": true,
    "coc.preferences.formatOnType": true,
    "css.hover.references": true,
    "emmet.includeLanguages": {
        "blade": "html",
        "vue-html": "html",
        "javascript": "javascriptreact",
        "htmldjango": "html"
    },
    "snippets.ultisnips.pythonPrompt": true
}


